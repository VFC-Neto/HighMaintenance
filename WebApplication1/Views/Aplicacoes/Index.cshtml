        <th>
            @Html.DisplayNameFor(model => model.APP_RENTABILIDADE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.APP_VALORMINIMO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.APP_VALORMAXIMO)
        </th>
